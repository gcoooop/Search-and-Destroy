{
  "name": "Search and Destroy",
  "version": "2.0",
  "description": "To oblivion with you heathen!",
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["./utils/storage_utils.js", "./utils/utils.js", "./engine/dynamic_array.js", "./engine/operation.js", "./engine/component_targeter.js", "./engine/page_targeter.js", "./engine/targeting_engine.js", "main.js"],
      "css": ["main.css"],
      "run_at": "document_end"
    }
  ],
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "manifest_version": 3
}